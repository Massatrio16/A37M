	-r /system/app/mcRegistry/020f0000000000000000000000000000.tlbin \
	-r /system/app/mcRegistry/030b0000000000000000000000000000.drbin \
	-r /system/app/mcRegistry/030b0000000000000000000000000000.tlbin \
	-r /system/app/mcRegistry/37b404db11db4fd4af5e846106b1c3d0.tlbin \
	-r /system/app/mcRegistry/070b0000000000000000000000000000.drbin \
	-r /system/app/mcRegistry/070b0000000000000000000000000000.tlbin \
	-r /system/app/mcRegistry/070c0000000000000000000000000000.drbin \
	-r /system/app/mcRegistry/070c0000000000000000000000000000.tlbin \
	-r /system/app/mcRegistry/070c0000000000000000000000000001.drbin \
	-r /system/app/mcRegistry/070c0000000000000000000000000001.tlbin \
