	-r /system/app/mcRegistry/9073f03a9618383bb1856eb3f990babd.tlbin \
	-r /system/app/mcRegistry/40188311faf343488db888ad39496f9a.drbin \
	-r /system/app/mcRegistry/40188311faf343488db888ad39496f9a.tlbin \
	-r /system/app/mcRegistry/0706000000000000000000000000004d.tlbin \
	-r /system/app/mcRegistry/04010000000000000000000000000000.tlbin \
